{"css":".flickity-fullscreen {\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  text-align: center;\n  /* Flexbox-Container-Settings */\n  /* dots are lines */\n}\n.flickity-fullscreen img {\n  max-width: 100%;\n  max-height: 80%;\n  object-fit: contain;\n  height: auto;\n  width: auto;\n}\n.flickity-fullscreen h3 {\n  margin: 0.5em auto 0.5 auto;\n  max-width: 50em;\n}\n.flickity-fullscreen h1,\n.flickity-fullscreen h2,\n.flickity-fullscreen h3,\n.flickity-fullscreen h4,\n.flickity-fullscreen h5 {\n  margin: 0 0 0.5em 0;\n}\n.flickity-fullscreen .flickity-viewport {\n  width: 100%!important;\n  height: 100%!important;\n}\n.flickity-fullscreen .gallery-cell {\n  width: 100%;\n  height: 100%;\n  padding: 40px;\n  overflow-y: auto;\n  display: flex;\n  flex-direction: column;\n  flex-wrap: nowrap;\n  justify-content: center;\n  align-content: stretch;\n  align-items: center;\n}\n.flickity-fullscreen .gallery-cell > div {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  justify-content: center;\n  align-content: center;\n  align-items: center;\n  height: 100%;\n  width: 100%;\n  overflow: hidden;\n}\n.flickity-fullscreen .gallery-cell > div img {\n  flex: 0 1 auto;\n  max-width: 48% !important;\n  max-height: 48% !important;\n  margin: 1%;\n}\n.flickity-fullscreen .flickity-page-dots {\n  bottom: -22px;\n}\n.flickity-fullscreen .flickity-page-dots .dot {\n  height: 4px;\n  width: 40px;\n  margin: 0;\n  border-radius: 0;\n}\n.flickity-slider {\n  height: 100vh;\n  width: 100%;\n}\n.bottom-bar {\n  background: rgba(12, 12, 12, 0.2);\n  color: #fff;\n}\n.bottom-inner {\n  padding-left: 30px;\n  padding-top: 15px;\n  padding-bottom: 15px;\n  text-align: center;\n}\n","sourceMap":{"version":3,"sources":["imports/ui/stylingsheets/bigPicture.less"],"names":[],"mappings":"AAAA;EACE,kBAAA;EACA,MAAA;EACA,OAAA;EACA,QAAA;EACA,SAAA;EACA,kBAAA;;;;AANF,oBAOE;EAEE,eAAA;EACA,eAAA;EAEA,mBAAA;EACA,YAAA;EACA,WAAA;;AAdJ,oBAgBE;EACE,2BAAA;EACA,eAAA